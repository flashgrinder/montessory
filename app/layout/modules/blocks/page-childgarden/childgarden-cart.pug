section.schoolCartBlock
	.wrapper
		.schoolCartContainer
			.schoolCart
				.schoolCart__header
					.schoolCart__starGreen.schoolCart__starGreen_one
						img(src="img/school-cart/objects/green/star-12.svg", alt="alt")
					.schoolCart__starGreen.schoolCart__starGreen_two
						img(src="img/school-cart/objects/green/star-13.svg", alt="alt")
					.schoolCart__starGreen.schoolCart__starGreen_three
						img(src="img/school-cart/objects/green/star-14.svg", alt="alt")
